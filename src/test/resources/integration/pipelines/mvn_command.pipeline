pipeline {
    agent any
    tools {
        jfrog 'jfrog-cli'
    }
    stages {
        stage ('Testing') {
            steps {
                writeFile file:'pom.xml', text:'<project><modelVersion>4.0.0</modelVersion><groupId>org.example</groupId><artifactId>untitled</artifactId><version>1.0-SNAPSHOT</version></project>'
                jf 'mvn-config'
                sh 'mvn -v'
            }
        }
    }
}